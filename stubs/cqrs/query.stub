<?php

namespace App\Queries;

use App\Models\{{ name }};

class {{ name }}Query
{
    public function findById($id)
    {
        $model = {{ name }}::query()->find($id);

        $cachedData = $model->getCacheData();

        if ($cachedData) return $cachedData;

        $model->setCacheData();

        return $model;
    }

    public function getAll()
    {
        return {{ name }}::all();
    }
}
